{
  "name": "helper-related",
  "description": "Template helper for generating a list of links to the homepages of related GitHub/npm projects.",
  "version": "0.13.1",
  "homepage": "https://github.com/helpers/helper-related",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "helpers/helper-related",
  "bugs": {
    "url": "https://github.com/helpers/helper-related/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ansi-green": "^0.1.1",
    "arr-filter": "^1.1.1",
    "async-array-reduce": "^0.1.0",
    "extend-shallow": "^2.0.1",
    "get-pkgs": "^0.4.0",
    "get-value": "^2.0.3",
    "is-valid-glob": "^0.3.0",
    "lazy-cache": "^1.0.3",
    "success-symbol": "^0.1.0"
  },
  "devDependencies": {
    "assemble-core": "^0.12.0",
    "engine-base": "^0.1.2",
    "engine-handlebars": "^0.8.0",
    "gulp-format-md": "^0.1.7",
    "mocha": "*",
    "should": "*"
  },
  "keywords": [
    "async",
    "github",
    "helper",
    "homepage",
    "links",
    "list",
    "npm",
    "related",
    "repo",
    "repos",
    "resolve",
    "template"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "handlebars-helpers",
        "helper-reflinks",
        "template-helpers",
        "verb",
        "remarkable",
        "micromatch"
      ]
    },
    "reflinks": [
      "assemble",
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}